{
  "name": "mosaic-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n web,api \"vite\", \"nodemon backend/server.js\"",
    "dev:web": "vite",
    "dev:server": "node backend/server.js",
    "build": "npm run build:web && npm run build:api",
    "build:web": "vite build",
    "build:api": "tsc -p backend/tsconfig.json",
    "start": "node backend/dist/server.js",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0",
    "test": "vitest run",
    "test:e2e": "playwright test --reporter=html,line --timeout=120000",
    "e2e:install": "npx playwright install chromium firefox",
    "e2e:debug": "PWDEBUG=1 playwright test --project=chromium",
    "e2e:report": "zip -r playwright-artifacts.zip playwright-report test-results",
    "patch": "node scripts/openaiPatch.js",
    "scaffold": "node scripts/setupStructure.cjs",
    "ci": "bash scripts/ci.sh",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:studio": "prisma studio",
    "prisma:validate": "prisma validate",
    "db:status": "prisma migrate status",
    "db:dev": "prisma migrate dev --name init",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^4.32.6",
    "@tanstack/react-router": "^1.47.1",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^6.11.2",
    "framer-motion": "^12.23.12",
    "fs-extra": "^11.3.0",
    "helmet": "^8.1.0",
    "image-size": "^1.0.1",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "latest": "^0.2.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "multer": "^1.4.5-lts.1",
    "ngeohash": "^0.6.3",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.3",
    "pdfkit": "^0.13.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.8.0",
    "recharts": "^2.15.4",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "stripe": "^11.18.0",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
