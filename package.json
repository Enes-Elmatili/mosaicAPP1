    "postcss": "^8.5.6",
    "prisma": "^6.14.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-pwa": "^1.0.2",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
